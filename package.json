{
	"name": "playwright-test-automation",
	"version": "1.0.0",
	"description": "Playwright Test Runner with JavaScript",
	"main": "index.js",
	"scripts": {
		"test": "npx playwright test",
		"debug-all": "npx playwright test --debug",
